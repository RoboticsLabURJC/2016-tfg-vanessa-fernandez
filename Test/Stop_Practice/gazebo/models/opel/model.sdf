<?xml version='1.0'?>
<sdf version="1.4">
<model name="roto">
  <pose>0 0 0.5 0 0 0</pose>
    <link name="link">
        <gravity>1</gravity>
        <inertial>
            <mass>500</mass>
        </inertial>
    <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://opel/meshes/model.dae</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://opel/meshes/model.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    <plugin name="miniCarMotors" filename="libminiCarMotors.so"/>
    
    <link name='camera'>
        <pose>0.30000 -0.5000 1.05 0.000000 0.000000 0.000000</pose>
        
        <sensor name='cam_f1_left' type='camera'>
          <pose>0.000000 0.000000 0.000000  0.00000 0.00000 -1.57000</pose>
          <update_rate>20.000000</update_rate>
          <camera name='__default__'>
            <horizontal_fov>1.570000</horizontal_fov>
            <image>
              <width>640</width>
              <height>480</height>
              <format>R8G8B8</format>
          </image>
            <clip>
              <near>0.100000</near>
              <far>15.000000</far>
          </clip>
        </camera>
          <plugin name='camera_dump' filename='libcamera_dumpF1.so'/>
      </sensor>
        <visual name='visual'>
          <geometry>
<sphere>
                          <radius>.01</radius>
</sphere>
        </geometry>
      </visual>
        <collision name='collision'>
          <geometry>
            <box>
              <size>0.100000 0.100000 0.100000</size>
          </box>
        </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100000.000000</mu>
                <mu2>100000.000000</mu2>
                <slip1>0.000000</slip1>
                <slip2>0.000000</slip2>
            </ode>
          </friction>
            <bounce/>
            <contact>
              <ode/>
          </contact>
        </surface>
          <max_contacts>10</max_contacts>
      </collision>
        <velocity_decay>
          <linear>0.000000</linear>
          <angular>0.000000</angular>
      </velocity_decay>
        <self_collide>0</self_collide>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
    </link>
    
    <include>
      <uri>model://kinect</uri>
      <pose>0.30000 -1.7000 0.5 0.000000 0.000000 -1.57</pose>
    </include>
    
    <plugin name="pose3d" filename="libpose3dF1.so"/>
    
    <joint type="revolute" name="camera">
        <pose>0 0 0 0 0 0</pose>
        <child>camera</child>
        <parent>link</parent>
        <axis>
          <xyz>0 0 0</xyz>
      </axis>
    </joint>
    
    <joint type="revolute" name="kinect">
        <pose>0 0 0 0 0 0</pose>
        <child>kinect::link</child>
        <parent>link</parent>
        <axis>
          <xyz>0 0 0</xyz>
      </axis>
    </joint>
  </model>
</sdf>
